{
  "check-coverage": true,
  "extension": [".js"],
  "exclude": [
    "test",
    "lib/**/*-test.js"
  ],
  "statements": 10,
  "branches": 10,
  "functions": 10,
  "lines": 10,
  "reporter": ["lcov", "html", "text", "text-summary"],
  "watermarks": {
    "lines": [10, 99],
    "functions": [10, 99],
    "branches": [10, 99],
    "statements": [10, 99]
  }
}
